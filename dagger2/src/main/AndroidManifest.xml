<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ray.demo.dagger2">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".app.view.DraggerActivity"
            android:label="@string/app_name_dagger"
            android:theme="@style/Base.Theme.AppCompat.Light">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.ray.demo.RAY_DEMO" />
            </intent-filter>
        </activity>
        <activity
            android:name=".app.view.UserDetailActivity"
            android:label="@string/app_name_dagger"
            android:theme="@style/Base.Theme.AppCompat.Light" />

        <activity
            android:name=".app.view.SplashActivity"
            android:label="@string/app_name_dagger"
            android:theme="@style/Base.Theme.AppCompat.Light"></activity>

        <activity
            android:name=".app.view.SubCompActivity"
            android:label="Dagger_SubComp"
            android:theme="@style/Base.Theme.AppCompat.Light">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.ray.demo.RAY_DEMO" />
            </intent-filter>
        </activity>
    </application>

</manifest>
